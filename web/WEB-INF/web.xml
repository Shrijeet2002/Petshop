<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>api.login</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>registration</servlet-name>
        <servlet-class>api.registration</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>home</servlet-name>
        <servlet-class>api.home</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DelAdmin</servlet-name>
        <servlet-class>DelAdmin</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SessionLogOut</servlet-name>
        <servlet-class>api.SessionLogOut</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ValidateEmail</servlet-name>
        <servlet-class>api.ValidateEmail</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>pwd</servlet-name>
        <servlet-class>api.pwd</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>order</servlet-name>
        <servlet-class>api.order</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>registration</servlet-name>
        <url-pattern>/registration</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>home</servlet-name>
        <url-pattern>/home</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DelAdmin</servlet-name>
        <url-pattern>/DelAdmin</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SessionLogOut</servlet-name>
        <url-pattern>/SessionLogOut</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ValidateEmail</servlet-name>
        <url-pattern>/ValidateEmail</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>pwd</servlet-name>
        <url-pattern>/pwd</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>order</servlet-name>
        <url-pattern>/order</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
